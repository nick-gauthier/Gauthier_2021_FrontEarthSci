isbn = {4955139574},
issn = {17550998},
journal = {Molecular Ecology Resources},
keywords = {Mantel test,landscape genetics,linear mixed effects model,model selection,regression on distance matrices,simulation},
number = {1},
pages = {55--67},
pmid = {28199780},
primaryClass = {arXiv:physics},
title = {{A comparison of regression methods for model selection in individual-based landscape genetic analysis}},
volume = {18},
year = {2018}
}
