journal = {Ecological Modelling},
keywords = {Agent-based model,Cooperation,Heterogeneous landscape,Mobility,Social-ecological systems,Spatial heterogeneity},
number = {January},
pages = {1--11},
pmid = {15002778},
title = {{The effect of spatial heterogeneity and mobility on the performance of social-ecological systems}},
