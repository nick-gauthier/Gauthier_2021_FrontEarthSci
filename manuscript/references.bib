keywords = {Ancestral Pueblo,Hopi,crop modeling,experimental farming,maize agriculture},
number = {2},
pages = {282--307},
title = {{Comparing Maize Paleoproduction Models with Experimental Data}},
url = {http://www.bioone.org/doi/pdf/10.2993/0278-0771-37.2.282},
volume = {37},
year = {2017}
