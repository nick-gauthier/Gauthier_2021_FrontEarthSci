title = {{Amplification of the North American "Dust Bowl" drought through human-induced land degradation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19289836 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2664008},
