file = {:home/nick/gdrive/Mendeley Desktop/Comrie, Glenn - 1999 - Principal components-based regionalization of precipitation regimes across the southwest United States and northe.pdf:pdf},
isbn = {0936-577X},
issn = {0936577X},
journal = {Climate Research},
keywords = {Climate regionalization,Monsoon variability,Northern Mexico,Precipitation,Principal components analysis,Southwest United States},
number = {3},
pages = {201--215},
title = {{Principal components-based regionalization of precipitation regimes across the southwest United States and northern Mexico, with an application to monsoon precipitation variability}},
volume = {10},
year = {1999}
}
