@article{Hangal2010,
abstract = {When searching for a person in an online social network, most contemporary networks return source-target paths, ranked only by degrees of separation. Not only does this fail to re ect social tie strength (how well two people know one another), it also does not model asymmetry in social relationships (i.e. just because one person pays attention to another, it does not mean the latter will reciprocate). We propose that search in social networks can be made more e ective by incorporating weighted and directed inuence edges in the social graph, thus capturing both tie strength and asymmetry. Formally, the in uence a person A has over person B is de ned as the proportion of B's investments B makes on A. We study two large real-world networks, DBLP (a computer science bibliography network) and a network formed by one month of Twitter retweet data. Our experiments show that for these social networks, the best paths according to our in uence metric are not necessarily the shortest paths: a longer path is better in 68{\%} of searches in Twitter and 45{\%} of searches in DBLP. Furthermore, even when the best and shortest path lengths are equal, we nd that the best path is often better than a random shortest path of the same length by a signicant margin.},
author = {Hangal, Sudheendra and MacLean, Diana and Lam, Monica s. and Heer, Jeffrey},
file = {:home/nick/gdrive/Mendeley Desktop/Hangal et al. - 2010 - All friends are not equal Using weights in social graphs to improve search.pdf:pdf},
isbn = {9781450302258},
journal = {The 4th SNA-KDD Workshop '10},
title = {{All friends are not equal: Using weights in social graphs to improve search}},
url = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.298.1136},
volume = {10},
year = {2010}
}
